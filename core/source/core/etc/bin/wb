#!/bin/sh

WB_CLASSPATH=$WB_HOME/lib/ZD.Wildebeest.Core-@meta.project.version.full.dotted@.jar
WB_CLASSPATH=$WB_CLASSPATH:$WB_HOME/lib/mysql-connector-java-5.1.22.jar
